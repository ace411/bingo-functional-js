{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "no-undef": 0,
    "no-console": 0,
    "func-names": 0,
    "no-unused-vars": 0,
    "indent": ["error", 2]
  },
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true
  }
}